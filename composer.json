{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/annotations": "^1.13",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.11",
    "friendsofsymfony/rest-bundle": "^3.3",
    "gesdinet/jwt-refresh-token-bundle": "^1.0",
    "league/csv": "^9.8",
    "league/flysystem-bundle": "^2.2",
    "lexik/jwt-authentication-bundle": "^2.14",
    "nelmio/api-doc-bundle": "^4.8",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.2",
    "ramsey/uuid-doctrine": "^1.8",
    "sensio/framework-extra-bundle": "^6.2",
    "sonata-project/admin-bundle": "^4.9",
    "sonata-project/doctrine-orm-admin-bundle": "^4.2",
    "symfony/asset": "5.4.*",
    "symfony/console": "5.4.*",
    "symfony/doctrine-messenger": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/event-dispatcher": "5.4.*",
    "symfony/expression-language": "5.4.*",
    "symfony/flex": "^1.17|^2",
    "symfony/form": "5.4.*",
    "symfony/framework-bundle": "5.4.*",
    "symfony/http-client": "5.4.*",
    "symfony/intl": "5.4.*",
    "symfony/mailer": "5.4.*",
    "symfony/mime": "5.4.*",
    "symfony/monolog-bundle": "^3.7",
    "symfony/notifier": "5.4.*",
    "symfony/process": "5.4.*",
    "symfony/property-access": "5.4.*",
    "symfony/property-info": "5.4.*",
    "symfony/proxy-manager-bridge": "5.4.*",
    "symfony/runtime": "5.4.*",
    "symfony/security-bundle": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/string": "5.4.*",
    "symfony/translation": "5.4.*",
    "symfony/twig-bundle": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/web-link": "5.4.*",
    "symfony/webapp-meta": "^1.0",
    "symfony/webpack-encore-bundle": "^1.12",
    "symfony/yaml": "5.4.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "ekino/phpstan-banned-code": "^1.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.4",
    "phpstan/phpstan-doctrine": "^1.2",
    "phpstan/phpstan-symfony": "^1.1",
    "roave/security-advisories": "dev-latest",
    "symfony/browser-kit": "5.4.*",
    "symfony/css-selector": "5.4.*",
    "symfony/debug-bundle": "5.4.*",
    "symfony/maker-bundle": "^1.38",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/stopwatch": "5.4.*",
    "symfony/var-dumper": "5.4.*",
    "symfony/web-profiler-bundle": "5.4.*",
    "symplify/easy-coding-standard": "^10.1"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpstan/extension-installer": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "cs": [
      "vendor/bin/ecs check --ansi --no-interaction"
    ],
    "cs-fix": [
      "vendor/bin/ecs check --fix --ansi --no-interaction"
    ],
    "stan": [
      "vendor/bin/phpstan analyze --ansi --no-interaction"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.4.*"
    }
  },
  "minumum-stability": "dev"
}
